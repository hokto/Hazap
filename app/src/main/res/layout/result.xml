<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/Homemove_btn"
        android:layout_width="140dp"
        android:layout_height="75dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/button_state"
        android:text="ホームに戻る"
        android:textColor="#B9FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959" />

    <!-- <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/pie_chart"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />
-->
    <TextView
        android:id="@+id/Advice"
        android:layout_width="197dp"
        android:layout_height="143dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="152dp"
        android:text="主催者からのメッセージ"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/Homemove_btn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <RelativeLayout
        android:layout_width="408dp"
        android:layout_height="545dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/Map"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_marginTop="543dp"
            android:contentDescription="TODO"
            app:srcCompat="@drawable/back"
            tools:srcCompat="@drawable/back" />

        <ImageView
            android:id="@+id/Bestrefuge"
            android:layout_width="211dp"
            android:layout_height="222dp"
            android:layout_alignStart="@+id/Map"
            android:layout_alignLeft="@+id/Map"
            android:layout_alignTop="@+id/Map"
            android:layout_alignEnd="@+id/Map"
            android:layout_alignRight="@+id/Map"
            android:layout_alignBottom="@+id/Map"
            android:layout_marginStart="177dp"
            android:layout_marginLeft="177dp"
            android:layout_marginTop="226dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="97dp"
            android:contentDescription="TODO"
            app:srcCompat="@drawable/argoal"
            tools:srcCompat="@drawable/argoal" />

        <ImageView
            android:id="@+id/MyRefuge"
            android:layout_width="193dp"
            android:layout_height="213dp"
            android:layout_alignStart="@+id/Map"
            android:layout_alignLeft="@+id/Map"
            android:layout_alignTop="@+id/Map"
            android:layout_alignEnd="@+id/Map"
            android:layout_alignRight="@+id/Map"
            android:layout_alignBottom="@+id/Map"
            android:layout_marginStart="54dp"
            android:layout_marginLeft="54dp"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="161dp"
            android:layout_marginRight="161dp"
            android:layout_marginBottom="297dp"
            android:contentDescription="TODO"
            app:srcCompat="@drawable/arkeiyu"
            tools:srcCompat="@drawable/arkeiyu" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>